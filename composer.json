{
  "name": "secuconnect/secuconnect-php-sdk",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "swagger",
    "php",
    "sdk",
    "api"
  ],
  "homepage": "http://secuconnect.com",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "secuconnect KG",
      "homepage": "https://github.com/secuconnect/secuconnect-php-sdk"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/cache": "~1.0",
    "psr/log": "~1.1",
    "react/stomp": "~0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.5",
    "friendsofphp/php-cs-fixer": "~2.14"
  },
  "autoload": {
    "psr-4": {
      "Secuconnect\\Client\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Secuconnect\\Client\\": "test/"
    }
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml.dist"
  },
  "suggest": {
    "ext-stomp": "Needed to support SmartDevice behaviour"
  }
}
